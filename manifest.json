{
  "manifest_version": 3,
  "name": "自定义网页导航插件",
  "version": "1.0",
  "description": "允许用户自定义添加、删除和分类导航链接",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html" 
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["newtab.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "host_permissions": [
    "https://www.bing.com/*"
  ]
}